{
  "name": "@mover/frontend",
  "author": "Mover Systems ApS",
  "description": "Frontend for the Mover logistics platform.",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://thomas_darling@bitbucket.org/moversystems/platform.git"
  },
  "engines": {
    "node": ">=14.0.0 <15.0.0",
    "npm": ">=6.7.0 <7.0.0"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "not dead"
  ],
  "scripts": {
    "develop": "ts-node --project \"tools/tsconfig.json\" \"tools/scripts/develop\"",
    "clean": "rimraf \"artifacts/build\"",
    "build": "ts-node --project \"tools/tsconfig.json\" \"tools/scripts/build\"",
    "lint": "tslint --project \"tsconfig.json\"",
    "lint-all": "tslint --project \"tsconfig.json\" && stylelint \"src/**/*.css\" \"src/**/*.scss\" --config \"../stylelint.json\"",
    "translate-export": "ts-node --project \"tools/tsconfig.json\" \"tools/scripts/translate-export\"",
    "translate-pseudo": "ts-node --project \"tools/tsconfig.json\" \"tools/scripts/translate-pseudo\"",
    "translate-update": "ts-node --project \"tools/tsconfig.json\" \"tools/scripts/translate-update\"",
    "translate": "npm run translate-export && npm run translate-update && npm run translate-pseudo ",
    "rebuild-cloud": "npm run clean && npm run build && cd \"packages/cloud\" && npm run build && cd \"../..\"",
    "rebuild-desktop": "npm run clean && npm run build -- --platform=desktop --api=http://localhost:8080/api/ && cd \"packages/desktop\" && npm run build && cd \"../..\"",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@sentry/browser": "^5.27.3",
    "@sentry/integrations": "^5.27.3",
    "aurelia-animator-css": "^1.0.4",
    "aurelia-bootstrapper": "^2.3.3",
    "clone": "^2.1.2",
    "dropzone": "^5.7.2",
    "focus-trap": "^6.1.4",
    "gravatar-url": "^3.1.0",
    "inert-polyfill": "^0.2.5",
    "libphonenumber-js": "^1.9.3",
    "luxon": "^1.25.0",
    "njwt": "^1.0.0",
    "popper.js": "^1.16.1",
    "tabbable": "^5.1.3",
    "tslib": "^2.0.3",
    "uuid": "^8.3.1"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@types/autoprefixer": "^9.7.2",
    "@types/clone": "^2.1.0",
    "@types/copy-webpack-plugin": "^6.2.0",
    "@types/dropzone": "^5.7.3",
    "@types/duplicate-package-checker-webpack-plugin": "^2.1.0",
    "@types/googlemaps": "^3.43.3",
    "@types/html-webpack-plugin": "^3.2.4",
    "@types/luxon": "^1.25.0",
    "@types/node": "^14.14.7",
    "@types/tabbable": "^3.1.0",
    "@types/uuid": "^8.3.0",
    "@types/webpack": "^4.41.25",
    "@types/webpack-bundle-analyzer": "^3.9.0",
    "@types/webpack-dev-server": "^3.11.1",
    "aurelia-webpack-plugin": "^4.0.0",
    "autoprefixer": "^10.0.2",
    "chalk": "^4.1.0",
    "commander": "^6.2.0",
    "copy-webpack-plugin": "^6.3.0",
    "css-loader": "^5.0.1",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "file-loader": "^6.2.0",
    "globs": "^0.1.4",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.0",
