{
  "name": "@mover/frontend",
  "author": "Mover Systems ApS",
  "description": "Frontend for the Mover logistics platform.",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://thomas_darling@bitbucket.org/moversystems/platform.git"
  },
  "engines": {
    "node": ">=16.0.0 <17.0.0",
    "npm": ">=8.0.0 <9.0.0"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "not dead"
  ],
  "scripts": {
    "develop": "ts-node --project \"tools/tsconfig.json\" \"tools/scripts/develop\"",
    "clean": "rimraf \"artifacts/build\"",
    "build": "ts-node --project \"tools/tsconfig.json\" \"tools/scripts/build\"",
    "lint": "tslint --project \"tsconfig.json\"",
    "lint-all": "tslint --project \"tsconfig.json\" && stylelint \"src/**/*.css\" \"src/**/*.scss\" --config \"../stylelint.json\"",
    "translate-export": "ts-node --project \"tools/tsconfig.json\" \"tools/scripts/translate-export\"",
    "translate-pseudo": "ts-node --project \"tools/tsconfig.json\" \"tools/scripts/translate-pseudo\"",
    "translate-push-phrase": "ts-node --project \"tools/tsconfig.json\" \"tools/scripts/translate-push-phrase\"",
    "translate-pull-phrase": "ts-node --project \"tools/tsconfig.json\" \"tools/scripts/translate-pull-phrase\"",
    "translate": "npm run translate-export && npm run translate-update && npm run translate-pseudo ",
    "rebuild-cloud": "npm run clean && npm run build && cd \"packages/cloud\" && npm run build && cd \"../..\"",
    "rebuild-desktop": "npm run clean && npm run build -- --platform=desktop --api=http://localhost:8080/api/ && cd \"packages/desktop\" && npm run build && cd \"../..\"",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@sentry/browser": "^6.15.0",
    "@sentry/integrations": "^6.15.0",
    "aurelia-animator-css": "^1.0.4",
    "aurelia-bootstrapper": "^2.3.3",
    "clone": "^2.1.2",
    "color": "^4.0.1",
    "dropzone": "^5.7.2",
    "focus-trap": "^6.7.1",
    "gravatar-url": "^3.1.0",
    "inert-polyfill": "^0.2.5",
    "jwt-decode": "^3.1.2",
    "libphonenumber-js": "^1.9.42",
    "luxon": "^2.4.0",
    "popper.js": "^1.16.1",
    "tabbable": "^5.2.1",
    "tslib": "^2.3.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@cerner/duplicate-package-checker-webpack-plugin": "^2.1.0",
    "@types/clone": "^2.1.0",
    "@types/color": "^3.0.2",
    "@types/copy-webpack-plugin": "^8.0.1",
    "@types/dropzone": "^5.7.3",
    "@types/googlemaps": "^3.43.3",
    "@types/html-webpack-plugin": "^3.2.6",
    "@types/luxon": "^2.3.2",
    "@types/node": "^16.11.7",
    "@types/tabbable": "^3.1.1",
    "@types/uuid": "^8.3.1",
    "@types/webpack": "^5.28.0",
    "@types/webpack-bundle-analyzer": "^4.4.1",
    "@types/webpack-dev-server": "^4.5.0",
    "@vue/preload-webpack-plugin": "^2.0.0",
    "aurelia-webpack-plugin": "^5.0.3",
    "autoprefixer": "^10.4.0",
    "chalk": "^4.1.2",
    "commander": "^8.3.0",
    "copy-webpack-plugin": "^9.1.0",
    "css-loader": "^6.5.1",
    "file-loader": "^6.2.0",
    "globs": "^0.1.4",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "html-loader": "^3.0.1",
    "html-webpack-plugin": "^5.5.0",
    "patch-package": "^6.2.2",
    "pkg-dir": "^5.0.0",
    "postcss": "^8.3.11",
    "postcss-loader": "^6.2.0",
    "rimraf": "^3.0.2",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.1.0",
    "stylelint-config-standard-scss": "^2.0.1",
    "stylelint-scss": "^4.0.0",
    "svg-sprite-loader": "^6.0.11",
    "translation-loader": "^1.1.2",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "typescript": "^4.5.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.64.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-dev-server": "^4.5.0",
    "worker-loader": "^3.0.8"
  }
}
