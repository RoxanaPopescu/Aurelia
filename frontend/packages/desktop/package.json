{
  "name": "@mover/frontend-desktop",
  "author": "Thomas Darling",
  "description": "Frontend for the Mover logistics platform, packaged as a desktop app.",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://thomas_darling@bitbucket.org/moversystems/platform.git"
  },
  "engines": {
    "node": ">=10.13.0 <14.0.0",
    "npm": ">=6.7.0 <7.0.0"
  },
  "main": "artifacts/build/index.js",
  "scripts": {
    "postinstall": "cross-env electron-builder install-app-deps",
    "build": "cross-env rimraf 'artifacts/{build,client}' && cpx '../../artifacts/build/**' 'artifacts/build/client' && tsc",
    "start": "cross-env electron 'artifacts/build/' --debug",
    "lint": "cross-env tslint --project 'tsconfig.json'",
    "release": "cross-env rimraf 'artifacts/release' && electron-builder --config 'tools/release.yml'"
  },
  "dependencies": {},
  "peerDependencies": {},
  "devDependencies": {
    "@types/node": "^13.5.0",
    "cpx": "^1.5.0",
    "cross-env": "^6.0.3",
    "electron": "^7.1.10",
    "electron-builder": "^22.2.0",
    "nodemon": "^2.0.2",
    "rimraf": "^3.0.0",
    "ts-node": "^8.6.2",
    "tslib": "^1.10.0",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5"
  }
}
