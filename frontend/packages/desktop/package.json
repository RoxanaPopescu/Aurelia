{
  "name": "@mover/frontend-desktop",
  "author": "Mover Systems ApS",
  "productName": "Mover",
  "description": "Frontend for the Mover logistics platform, packaged as a desktop app.",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://thomas_darling@bitbucket.org/moversystems/platform.git"
  },
  "engines": {
    "node": ">=10.13.0 <14.0.0",
    "npm": ">=6.7.0 <7.0.0"
  },
  "main": "artifacts/build/index.js",
  "scripts": {
    "postinstall": "cross-env electron-builder install-app-deps",
    "build": "cross-env rimraf \"artifacts/{build}\" && cpx \"../../artifacts/build/**\" \"artifacts/build/frontend\" && tsc",
    "start": "cross-env electron \"artifacts/build/\" --debug",
    "lint": "cross-env tslint --project \"tsconfig.json\"",
    "package": "cross-env rimraf 'artifacts/package' && electron-builder --publish=never --config 'tools/package.yml' -mwl"
  },
  "dependencies": {
    "electron-pdf-window": "^1.0.12",
    "tslib": "^2.0.1"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@types/node": "^14.6.1",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.2",
    "electron": "^10.0.1",
    "electron-builder": "^22.8.0",
    "electron-pdf-window": "^1.0.12",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.2"
  }
}
