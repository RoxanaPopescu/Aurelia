{
  "name": "@mover/frontend-desktop",
  "author": "Thomas Darling",
  "description": "Frontend for the Mover logistics platform, packaged as a desktop app.",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://thomas_darling@bitbucket.org/moversystems/platform.git"
  },
  "engines": {
    "node": ">=10.13.0 <12.0.0",
    "npm": ">=6.7.0 <7.0.0"
  },
  "main": "artifacts/build/index.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "build": "rimraf 'artifacts/{build,client}' && cpx '../../artifacts/build/**' 'artifacts/client' && tsc",
    "start": "electron 'artifacts/build/' --debug",
    "lint": "tslint --project 'tsconfig.json' --format stylish",
    "release": "electron-builder --config 'tools/release.yml'"
  },
  "dependencies": {},
  "peerDependencies": {},
  "devDependencies": {
    "@types/compression": "0.0.36",
    "@types/express": "^4.16.1",
    "@types/luxon": "^1.11.0",
    "@types/node": "^10.12.24",
    "@types/uuid": "^3.4.4",
    "cpx": "^1.5.0",
    "electron": "^4.0.4",
    "electron-builder": "^20.38.5",
    "nodemon": "^1.18.10",
    "rimraf": "^2.6.3",
    "ts-node": "8.0.2",
    "tslib": "^1.9.3",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3"
  }
}
