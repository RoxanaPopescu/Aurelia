{
  "name": "@mover/frontend-desktop",
  "author": "Thomas Darling",
  "description": "Frontend for the Mover logistics platform, packaged as a desktop app.",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://thomas_darling@bitbucket.org/moversystems/platform.git"
  },
  "engines": {
    "node": ">=10.13.0 <14.0.0",
    "npm": ">=6.7.0 <7.0.0"
  },
  "main": "artifacts/build/index.js",
  "scripts": {
    "postinstall": "cross-env electron-builder install-app-deps",
    "build": "cross-env rimraf \"artifacts/{build,client}\" && cpx \"../../artifacts/build/**\" \"artifacts/build/client\" && tsc",
    "start": "cross-env electron \"artifacts/build/\" --debug",
    "lint": "cross-env tslint --project \"tsconfig.json\"",
    "release-all": "cross-env rimraf \"artifacts/release\" && electron-builder --config \"tools/release.yml\" --fml",
    "release-osx": "cross-env rimraf \"artifacts/release\" && electron-builder --config \"tools/release.yml\"",
    "release-win": "cross-env rimraf \"artifacts/release\" && electron-builder --config \"tools/release.yml\" --win"
  },
  "dependencies": {},
  "peerDependencies": {},
  "devDependencies": {
    "@types/node": "^14.6.0",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.2",
    "electron": "^10.0.0",
    "electron-builder": "^22.8.0",
    "electron-pdf-window": "^1.0.12",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "tslib": "^2.0.1",
    "tslint": "^6.0.0",
    "typescript": "^4.0.2"
  }
}
