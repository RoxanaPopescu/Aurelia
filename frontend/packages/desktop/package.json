{
  "name": "@mover/frontend-desktop",
  "author": "Mover Systems ApS",
  "productName": "Mover",
  "description": "Frontend for the Mover logistics platform, packaged as a desktop app.",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://thomas_darling@bitbucket.org/moversystems/platform.git"
  },
  "engines": {
    "node": ">=16.0.0 <17.0.0",
    "npm": ">=8.0.0 <9.0.0"
  },
  "main": "artifacts/build/index.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "build": "rimraf \"artifacts/{build}\" && cpx \"../../artifacts/build/**\" \"artifacts/build/frontend\" && tsc",
    "start": "electron \"artifacts/build/\" --debug",
    "lint": "tslint --project \"tsconfig.json\"",
    "package": "rimraf 'artifacts/package' && electron-builder --publish=never --config 'tools/package.yml' -mwl"
  },
  "dependencies": {
    "electron-pdf-window": "^1.0.12",
    "tslib": "^2.3.1"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@types/node": "^16.11.7",
    "cpx2": "^4.0.0",
    "electron": "^10.1.5",
    "electron-builder": "^22.9.1",
    "electron-pdf-window": "^1.0.12",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "typescript": "^4.4.4"
  }
}
