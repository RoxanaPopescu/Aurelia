    "translate-pseudo": "ts-node --project \"tools/tsconfig.json\" \"tools/scripts/translate-pseudo\"",
    "translate": "npm run translate-export && ts-node --project \"tools/tsconfig.json\" \"tools/scripts/translate-update\""
  },
