  "scripts": {
    "develop": "rimraf \"artifacts/build\" && tsc --watch",
    "build": "rimraf \"artifacts/build\" && tsc",
    "start": "node \"artifacts/build/index\"",
    "lint": "tslint --project \"tsconfig.json\"",
